<%= render 'shared/nav' %>
<div class="page">
  <h1 class="chart_page_title"><%= @coin.name %></h1>
  <div id="charts">
    <div class="chart" id="avg">
      <%= render_avg_price_chart(@coin) %>
    </div>
    <div class="chart" id="ask">
      <%= render_ask_price_chart(@coin) %>
    </div>
    <div class="chart" id="bid">
      <%= render_bid_price_chart(@coin) %>
    </div>
  </div>
</div>