<div id="stoch-draggable-resizable" class="card text-center shadow indicator-card">
   <div id="stoch-card-title" class="card-header stoch text-white op-card-header"> 
      <div style="flex:1"></div>
      <div class="op-card-title">
        <%= "stoch".upcase %>
      </div> 
      <div class="remove">
        <button id="remove-link" class="remove-link" onclick="remove(event)">
         <i class="far fa-window-close" style="color:white"></i>
        </button>
      </div>
    </div>
  <div class="indicator-body">
    <%= collection_select :coin, :coin_id, Coin.all.order(:name), :id, :name, :include_blank => true , class: "browser-default custom-select"%>
    <div class="input-row">
      <div class="form-outline">
        <input type="number" id="valueK" class="form-control" />
        <label class="form-label" for="typeNumber" style="font-size:5">Lower (USD)</label>
      </div>
      <div class="form-outline">
        <input type="number" id="valueKError" class="form-control" />
        <label class="form-label" for="typeNumber">Error (USD)</label>
      </div>
    </div>
    <div class="input-row">
      <div class="form-outline">
        <input type="number" id="valueD" class="form-control" />
        <label class="form-label" for="typeNumber" style="font-size:5">Upper (USD)</label>
      </div>
      <div class="form-outline">
        <input type="number" id="valueDError" class="form-control" />
        <label class="form-label" for="typeNumber">Error (USD)</label>
      </div>
    </div>
    <select id="interval" class="mdb-select md-form">
      <option value="" disabled selected>Interval</option>
      <% %w(1m 5m 15m 30m 1h 2h 4h 12h 1d 1w).each_with_index do |interval, index| %>
        <option value="<%= index %>"><%= interval %></option>
      <% end %>
    </select>
  </div>
</div>